CREATE TABLE Article (
    title TEXT NOT NULL,
    desc TEXT,
    date TEXT NOT NULL,
    imageUrl TEXT,
    url TEXT NOT NULL,
    authorId TEXT,
    authorName TEXT NOT NULL
);

insertArticle:
INSERT INTO Article(title, desc, date, imageUrl, url, authorId, authorName)
VALUES(?,?,?,?, ?, ?, ?);

selectAllArticles:
SELECT Article.* FROM Article;

removeAllArticles:
DELETE FROM Article;